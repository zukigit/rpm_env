    <UserControl x:Class="jp.co.ftf.jobcontroller.JobController.Form.JobEdit.FCopySample"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:src="clr-namespace:jp.co.ftf.jobcontroller.JobController"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             FontSize="11" FontFamily="Meiryo">
    <Canvas x:Name="LayoutRoot" Background="Transparent" >

        <Path Stroke="Black" Fill="White"  StrokeThickness="1">
            <Path.Data>
                <PathGeometry>
                    <PathFigure StartPoint="0,0" IsClosed="False">
                        <LineSegment Point="75,0"/>
                        <LineSegment Point="75,23"/>
                        <BezierSegment Point1="42,20" Point2="13,29" Point3="0,24"/>
                        <LineSegment Point="0,0"/>
                    </PathFigure>
                </PathGeometry>
            </Path.Data>
        </Path>
        <Path Stroke="Black" Fill="White"  StrokeThickness="1">
            <Path.Data>
                <PathGeometry>
                    <PathFigure StartPoint="57,19.5" IsClosed="False">
                        <LineSegment Point="66,19.5"/>
                        <LineSegment Point="66,16"/>
                        <LineSegment Point="72,23"/>
                        <LineSegment Point="66,30"/>
                        <LineSegment Point="66,26.5"/>
                        <LineSegment Point="57,26.5"/>
                        <LineSegment Point="57,19"/>
                    </PathFigure>
                </PathGeometry>
            </Path.Data>
        </Path>

        <TextBlock   Canvas.ZIndex="101" Name="tbJobName" Foreground="Black" FontSize="10.5" Text="{x:Static src:Properties.Resources.file_copy_icon_label_text}"
                     VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap"  MaxWidth="90"
                     Height="15" Width="70"  Canvas.Left="4" Canvas.Top="5"
                     FontWeight="Normal" TextAlignment="Center">
        </TextBlock>

    </Canvas>
</UserControl>
